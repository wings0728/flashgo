<launch>
  <node name="flashgo_node"  pkg="flashgo"  type="flashgo_node" output="screen">
    <param name="port"         type="string" value="/dev/ttyFlash"/>  
    <param name="baudrate"     type="int"    value="115200"/>
    <param name="frame_id"     type="string" value="lidar"/>
    <param name="angle_fixed"  type="bool"   value="true"/>
    <param name="angle_min"    type="double" value="-65" />
    <param name="angle_max"    type="double" value="65" />
    <param name="ignore_array" type="string" value="" />
  </node>
</launch>
